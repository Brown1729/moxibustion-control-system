<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.smoxisys.mapper.TemperatureDataMapper">

    <resultMap id="BaseResultMap" type="com.smoxisys.domain.TemperatureData">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="patientName" column="patient_name" jdbcType="VARCHAR"/>
            <result property="temperature" column="temperature" jdbcType="DOUBLE"/>
            <result property="recordedTime" column="recorded_time" jdbcType="TIMESTAMP"/>
            <result property="patientId" column="patient_id" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,patient_name,temperature,
        recorded_time,patient_id
    </sql>
</mapper>
